    "@privy-io/node": "^0.6.2",
    "@quicknode/sdk": "^2.5.2",
    "axios": "^1.6.2",
    "bs58": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "ethers": "^5.7.2",
    "express": "^4.18.2",
    "geoip-lite": "^1.4.10",
    "ioredis": "^5.3.2",
    "pg": "^8.11.3",
    "request-ip": "^3.3.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/geoip-lite": "^1.4.4",
    "@types/node": "^20.10.5",
    "@types/pg": "^8.10.9",
    "@types/request-ip": "^0.0.41",
    "@types/supertest": "^6.0.3",
