{
  "name": "mevu",
  "version": "1.0.0",
  "description": "MEVU - Market Event Value Updater",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "start:prod": "node dist/index.js",
    "migrate": "tsx src/scripts/run-migrations.ts",
    "migrate:prod": "node dist/scripts/run-migrations.js",
    "swagger": "swagger-jsdoc -d swaggerDef.js src/routes/*.ts -o swagger.json",
    "download-logos": "tsx src/scripts/download-logos.ts",
    "test-teams-storage": "tsx src/scripts/test-teams-storage.ts",
    "loadtest:games-frontend": "tsx src/scripts/load-test-games-frontend.ts",
    "debug:ufc-moneyline": "tsx scripts/debug-ufc-moneyline.ts",
    "debug:ufc-moneyline:docker": "bash scripts/run-debug-ufc-moneyline.sh",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@balldontlie/sdk": "^1.2.2",
