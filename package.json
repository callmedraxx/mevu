    "@types/ws": "^8.5.10",
    "@vitest/coverage-v8": "^4.0.18",
    "supertest": "^7.2.2",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^4.0.18"
  }
